          <li class="head-item">Статус оплати: {{order_status}}</li>
          <li class="head-item">Id замовлення: {{order_id}}</li>
          <li class="head-item">Дата купівлі підписки: {{payment.datePay}}</li>
          <li class="head-item">Дата закінчення підписки:
            {{payment.dateEnd}}</li>
        {{/each}}
      </ul>
    {{else}}
      <p>Немає нічого</p>
    {{/if}}

