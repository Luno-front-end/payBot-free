    <h1 class="title">Інформація про підписників</h1>
  </div>
  <div>

    {{#if allUsers.length}}
      <ul class="head-list">

        {{#each allUsers}}
          <li class="head-item">Імʼя: {{first_name}}</li>
          <li class="head-item">Фамілія: {{last_name}}</li>
          <li class="head-item">Нікнейм: {{username}}</li>
          <li class="head-item">Пошта: {{payment.sender_email}}</li>
          <li class="head-item">Оплачено: {{pay}}</li>
